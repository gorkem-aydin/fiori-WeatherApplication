<mvc:View controllerName="mydemodemo.controller.View1" 

xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m"
	 xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified">
	<App>
		<pages>
			<Page title="Günlük Hava Durumu Uygulaması">
				<content>
			
			<ObjectHeader iconActive="false" id="__header0" intro="{/city}" introActive="true" number="" numberUnit="" title="{/country}"
						titleActive="true"/>
					<Table columns="{/cols}" id="__table0" items="{/items}">
						<customData>
							<sap.ui.core:CustomData id="__data2" key="sap-ui-fastnavgroup" value="true" writeToDom="true" xmlns:sap.ui.core="sap.ui.core"/>
						</customData>
						<items>
							<ColumnListItem counter="0" id="__item0">
								<cells>
									<Text maxLines="0" text="{date}"/>
									<Text maxLines="0" text="{temp}"/>
									<Text maxLines="0" text="{units}"/>
									<Text maxLines="0" text="{humidity}"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column>
								<header>
									<Label text="{name}"/>
								</header>
							</Column>
						</columns>
					</Table>
						
						
						<ObjectHeader iconActive="false" id="__header1" intro="{/city1}" introActive="true" number="" numberUnit="" title="{/country1}"
						titleActive="true"/>
					<Table columns="{/cols}" id="__table1" items="{/items1}">
						<customData>
							<sap.ui.core:CustomData id="__data3" key="sap-ui-fastnavgroup" value="true" writeToDom="true" xmlns:sap.ui.core="sap.ui.core"/>
						</customData>
						<items>
							<ColumnListItem counter="0" id="__item3">
								<cells>
									<Text maxLines="0" text="{date}"/>
									<Text maxLines="0" text="{temp}"/>
									<Text maxLines="0" text="{units}"/>
									<Text maxLines="0" text="{humidity}"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column>
								<header>
									<Label text="{name}"/>
								</header>
							</Column>
						</columns>
					</Table>
					
						<ObjectHeader iconActive="false" id="__header2" intro="{/city2}" introActive="true" number="" numberUnit="" title="{/country2}"
						titleActive="true"/>
					<Table columns="{/cols}" id="__table2" items="{/items2}">
						<customData>
							<sap.ui.core:CustomData id="__data4" key="sap-ui-fastnavgroup" value="true" writeToDom="true" xmlns:sap.ui.core="sap.ui.core"/>
						</customData>
						<items>
							<ColumnListItem counter="0" id="__item4">
								<cells>
									<Text maxLines="0" text="{date}"/>
									<Text maxLines="0" text="{temp}"/>
									<Text maxLines="0" text="{units}"/>
									<Text maxLines="0" text="{humidity}"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column>
								<header>
									<Label text="{name}"/>
								</header>
							</Column>
						</columns>
					</Table>
					
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>